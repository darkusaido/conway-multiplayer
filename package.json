{
  "name": "conway-multiplayer",
  "version": "0.0.0",
  "scripts": {
    "start": "node server/app.js",
    "postinstall": "./node_modules/bower/bin/bower install",
    "nodemon": "nodemon --debug server/app.js",
    "inspect": "node-inspector --no-preload",
    "dev-build-with-test": "ttab npm run test-server;ttab node-inspector;npm run nodemon",
    "dev-build": "ttab node-inspector --no-preload;npm run nodemon",
    "coffee": "coffee --watch --output spec/test_js --compile spec/test_coffee",
    "test-server": "jasmine-node spec --verbose --autotest --watch . --growl --coffee"
  },
  "dependencies": {
    "bower": "^1.4.1",
    "express": "^4.13.3",
    "lodash": "^3.10.1",
    "socket.io": "^1.4.4"
  },
  "devDependencies": {
    "jasmine-node": "^1.14.5",
    "node-inspector": "^0.12.3",
    "nodemon": "^1.4.0"
  }
}
